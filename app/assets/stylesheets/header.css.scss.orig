#hd-wrapper{
  background: url(header-bg.jpg) repeat-x;
}


#hd-wrapper nav{
  float: right;
  margin-top: 33px;
  display: inline;
}

nav ul.header-nav{
  list-style: none;
  margin: 0;
  padding: 0;
}

nav ul.header-nav li{
  float: left;
  margin-right: 5px;
  padding: 10px 44px;
}

nav ul.header-nav li:last-child{
  margin-right: 0;
}


nav ul.header-nav li a{
  font-weight: 300;
  color: #bf291f;
  text-decoration: none;
  font-size: 1em;

  -webkit-transition: 0.6s ease-out all;
  -moz-transition: 0.6s ease-out all;
  -ms-transition: 0.6s ease-out all;
  -o-transition: 0.6s ease-out all;
  transition: 0.6s ease-out all;
}

nav ul.header-nav li a:hover{
  text-decoration: none;
}

nav ul.header-nav li.underline:hover a:hover{
  text-decoration: underline !important;
}

